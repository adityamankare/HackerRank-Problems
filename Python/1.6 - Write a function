#---

def is_leap(year):    
    # Write your logic here
    if year >= 1900 and year <= 100000:
        if year%4 == 0 and (year%100 != 0 or year%400 ==0):
            return True
        else:
            return False
            

year = int(input())
print(is_leap(year))
